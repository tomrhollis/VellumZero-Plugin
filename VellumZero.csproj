<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>VellumZero</RootNamespace>
    <Version>0.10.0.0</Version>
    <Authors>Tom Hollis</Authors>
    <Product>VellumZero</Product>
    <RepositoryUrl>https://www.github.com/tomrhollis/VellumZero-Plugin</RepositoryUrl>
    <PackageId>VellumZero</PackageId>
    <PackageTags></PackageTags>
    <PlatformTarget>x64</PlatformTarget>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="refs\Discord.Net.Commands.dll" />
    <None Remove="refs\Discord.Net.Core.dll" />
    <None Remove="refs\Discord.Net.Rest.dll" />
    <None Remove="refs\Discord.Net.Webhook.dll" />
    <None Remove="refs\Discord.Net.WebSocket.dll" />
    <None Remove="refs\Microsoft.Data.Sqlite.dll" />
    <None Remove="refs\SQLitePCLRaw.core.dll" />
    <None Remove="refs\System.Collections.Immutable.dll" />
    <None Remove="refs\System.Interactive.Async.dll" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="refs\Discord.Net.Commands.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="refs\Discord.Net.Core.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="refs\Discord.Net.Rest.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="refs\Discord.Net.Webhook.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="refs\Discord.Net.WebSocket.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="refs\Microsoft.Data.Sqlite.dll" />
    <EmbeddedResource Include="refs\SQLitePCLRaw.core.dll" />
    <EmbeddedResource Include="refs\System.Collections.Immutable.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="refs\System.Interactive.Async.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Discord.Net" Version="2.2.0" />
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="3.1.7" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>


  <ItemGroup>
    <ProjectReference Include="..\vellum-for-EZ\src\vellum.csproj" />
  </ItemGroup>

</Project>
